
@model IEnumerable<StudentElection.Models.Categeory>
@using StudentElection.Models;

@{
    ViewBag.Title = "AnalyseVotePage";
}
<head>

    @Styles.Render("~/Content/css")

</head>
<body class="body-content">


    <h2>Analyse Vote</h2>
    <p>Elections Completed for below Categories</p>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <h4 style="color:white">Online Student Election System</h4>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Register Students", "Upload", "Admin")</li>
                    <li>@Html.ActionLink("Enroll Candidates", "Enroll", "Admin")</li>
                    <li>@Html.ActionLink("Launch Election", "Index", "CategoryList")</li>
                    <li>@Html.ActionLink("Analyse Votes", "AnalyseVote", "Admin")</li>
                    <li>@Html.ActionLink("LogOut", "LogOut", "Admin")</li>


                </ul>
            </div>
        </div>
    </div>
    <div>
        @foreach (var item in Model)
        {

            <ul>

                @*<li><a href="">@item.Name</a></li>*@
                <li style="color:white">@Html.ActionLink(item.Name, "GetStudentByCategory", new { Name = item.Name }, new { @class = "actionlinkcolor" })</li>


            </ul>
        }

        @{
            IEnumerable<Student> students = ViewData["Students"] as IEnumerable<Student>;
        }
        @if (students != null)
        {
            <table class="table">
                <tr>
                    <th>Roll No</th>
                    <th>Name</th>
                    <th>Branch</th>
                    <th>Year of Joining</th>
                    <th>Mobile Number</th>
                    <th>Votes</th>
                </tr>


                @foreach (Student st in students)
                {
                    <tr>
                        <td>@st.rollno</td>
                        <td>@st.studentname</td>
                        <td>@st.branch</td>
                        <td>@st.yearofjoining</td>
                        <td>@st.mobilenumber</td>
                        <td></td>
                    </tr>
                }


            </table>
            <a href="@Url.Action("Index","CategeoryDifference")" class="btn btn-primary btn-lg">Publish &raquo;</a>
        }
    </div>
</body>